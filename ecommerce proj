                                                  <div class="outer-checkbox">
                                                      <label class="st-flex st-selected">
                                                          <input type="checkbox" :value="selectedItem" v-model="filter.selected" @change="pushFilters(), filterAnalytics('left', filter.title, selectedItem)">
