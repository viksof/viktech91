    <script>
      

      
        var random = document.querySelector(".random-text");
        var button = document.querySelector(".button-us-link");
        function onRedirectionCheck(country_name) {
            if (country_name == 'United States') {
                setTimeout(
                    function open(event) {
                        if (document.querySelector(".display-none")) {
                            document.querySelector(".display-none").style.display = "block";
                          document.querySelector("body").style.overflow = "hidden";
                            random.innerHTML=" We noticed you’re shopping from USA";
                          button.innerText = "Continue to US Website"
                        }
                    },
                    1000
                )
            }
              else if(country_name != "India" && country_name != "United States"){
                 document.querySelector(".display-none").style.display = "block";
                document.querySelector("body").style.overflow = "hidden";
                 random.innerHTML= `We noticed you’re shopping from ${country_name}`;
                  button.innerText = "Continue to Global Website";
              }
            else { document.querySelector(".display-none").style.display = "none"; }
        }
      
     fetch('https://www.chumbak.com/browsing_context_suggestions.json')
        .then(data=>data.json())
        .then(result=> {
                console.log(result);
                onRedirectionCheck(result.detected_values.country_name);
            })
        
    </script>
 

