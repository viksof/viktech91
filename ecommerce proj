<script>
  var giftCheckAddedCount = 0;

  window.SLIDECART_OPENED = function(cart) {
    // Fires once the cart has loaded 
    if(document.getElementById('slidecarthq')){
      document.getElementById('slidecarthq').style.position='relative';
      document.getElementById('slidecarthq').style.zIndex='100';
    }
    
    //adding the checkbox
    if(giftCheckAddedCount == 0 ){
      console.log("Continue button added : 44");
      //adding the conitnue button 
      const conitnueButton = document.createElement("div");
      conitnueButton.className='button-continue';
      conitnueButton.innerHTML=`<a class="footer-continue" onclick="closeSlideCart(event)">Or continue shopping</a>`;
      document.querySelector('.custom-html').after(conitnueButton);
      console.log("Continue button added");
      
      giftCheckAddedCount=1;
    }   
}

  function closeSlideCart(event){
    window.SLIDECART_CLOSE();
  }

  function nativeCheckoutBegins(event){
    window.location = '/checkout';
    
  }
  
</script>
